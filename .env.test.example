# Environment: Test
# Copy this file to .env.test and fill in your values

# Supabase Configuration (Required for tests)
# Use test/mock values for testing
SUPABASE_URL="https://test-project.supabase.co"
SUPABASE_ANON_KEY="test-anon-key"
NEXT_PUBLIC_SUPABASE_URL="https://test-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="test-anon-key"

# Test environment flags
NODE_ENV="test"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Disable telemetry in tests
NEXT_TELEMETRY_DISABLED=1

# Optional: Enable test logging
# LOG_LEVEL="error"

# Optional: Mock external services (recommended)
MOCK_EXTERNAL_SERVICES=true

# Optional: Test timeout settings
# JEST_TIMEOUT=10000
# PLAYWRIGHT_TIMEOUT=30000
